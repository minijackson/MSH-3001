<<means global, echo=FALSE>>=
smartWordcloud <- function(texte, lang) {
  corpus <- VCorpus(VectorSource(texte))
  # Replace "'", ";" and "/" by spaces to separate words.
  corpus <- tm_map(corpus, content_transformer(function(x) {
                                                 gsub("'", " ", gsub(";", " ", gsub("/", " ", x)))
                                              })
                  )
  corpus <- tm_map(corpus, removePunctuation)
  corpus <- tm_map(corpus, removeWords, stopwords(lang))

  tdm <- TermDocumentMatrix(corpus)
  m <- as.matrix(tdm)
  v <- sort(rowSums(m), decreasing=TRUE)
  d <- data.frame(word=names(v), freq=v)

  wordcloud(d$word, d$freq, colors=brewer.pal(9, "PuBu")[4:9])
}
@

<<means fr, echo=FALSE, fig.cap="Nuage de mots du moyen de prise de conscience de l'existence de la C4 Cactus des personnes ayant répondu au sondage français", fig.pos="H">>=
smartWordcloud(sondage.fr$Si.oui..par.quels.moyens.en.avez.vous.entendu.parler.., "french")
@

<<means en, echo=FALSE, fig.cap="Nuage de mots du moyen de prise de conscience de l'existence de la C4 Cactus des personnes ayant répondu au sondage anglais", fig.pos="H">>=
smartWordcloud(sondage.en$If.you.are..by.which.means.have.you.heard.about.it., "english")
@

\paragraph{} Ce nuage de point nous démontre que la majorité des gens
connaissent la C4 Cactus grâce aux publicités, aux magasines et à Internet. En
ce qui concerne le sondage anglais, du fait qu'il ait été publié sur un site de
Citroën, une grande partie des personnes ont répondu qu'elles connaissent la C4
Cactus car elles suivent l'actualité de la marque.

