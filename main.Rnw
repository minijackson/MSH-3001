legendaryPie <- function(x) {
  x.table <- table(x)
  if(length(x.table) >= 3) {
    if(length(x.table) >= 9) {
      x.colors <- brewer.pal(length(x.table), "Set3")
    } else {
      x.colors <- brewer.pal(length(x.table), "Pastel1")
    }
  } else {
    x.colors <- brewer.pal(3, "Pastel1")
  }
  pie(x.table, col=x.colors)
  legend("topright", legend=x.table, fill=x.colors)
}
@

<<tranche age fr, echo=FALSE, fig.cap="Tranches d'âges des personnes ayant répondu au sondage français", fig.pos="H", fig.height=5>>=
